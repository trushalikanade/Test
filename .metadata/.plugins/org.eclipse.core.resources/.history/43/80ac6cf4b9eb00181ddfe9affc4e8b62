import java.util.Scanner;

public class anagram 
{
	public static String sort(String name)
	{
	  String this.name=name;
	  char temp;
	  char a[]= name.toCharArray();
	  for(int i=0;i<a.length-1;i++)
	  {
		  for(int j=j+1;j<a.length;j++)
		  {
			  if(a[i]>a[j])
			  {
				  temp=a[i];
				  a[i]=a[j];
				  a[j]=temp;
			  }
		  }
	  }
	  String fstring=new String(str)
			  return (fstring);
		
	}
}
