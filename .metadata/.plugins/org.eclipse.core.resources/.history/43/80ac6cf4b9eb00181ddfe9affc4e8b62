class hello
{
	 public static void main(String args[])
     {
		 anagram m= new anagram();
         String str1=m.sortstring("keep");
         String str2=m.sortstring("peek");
         
         if(str1.equals(str2))
         {
             System.out.println("anagram");
         }
         else
         {
             System.out.println("not anagram");

         }
     }
}
